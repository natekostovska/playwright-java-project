<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="Smoke" verbose="1" parallel="methods" thread-count="1">
    <listeners>
        <listener class-name="ui.utils.reports.allurereports.AllureTestListener"/>
        <listener class-name="ui.utils.reports.extentreports.TestListener"/>
    </listeners>
    <test name="Smoke">
        <groups>
            <run>
                <include name="smoke"/>
                <include name="loginCombinations"/>
            </run>
        </groups>
        <classes>
            <class name="ui.tests.LoginTests"/>
        </classes>
    </test>
</suite>